{
  "name": "bms-react-js-example",
  "version": "0.0.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "webpack -w --mode=development",
    "start": "concurrently \"cd .. && theme watch\" \"npm run watch\"",
    "test": "mocha --require @babel/register './{,!(node_modules)/**}/*.spec.js'",
    "BDD": "onchange ./**/*.js test/**/*.js -- npm test",
    "coverage": "nyc mocha --require @babel/register test/**/*",
    "noThemeWatch": "concurrently \"npm run watch\"",
    "format": "prettier --write \"./**/*.ts\" \"./**/*.js\" \"./**/*.scss\"",
    "prepublish": "npx shx cp ./package.json ./package-example.json",
    "build": "webpack --mode=production && npx bms-theme-kit -c"
  },
  "nyc": {
    "extension": [
      ".js",
      ".jsx"
    ],
    "include": [
      "./*"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/",
      "node_modules"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "all": true
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "devDependencies": {
    "@babel/core": "~7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "onchange": "^5.0.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "css-loader": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
